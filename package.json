{
  "name": "toggl-to-clockify-web",
  "version": "0.0.1",
  "description": "Web-based tool to transfer Toggl data to Clockify.",
  "main": "index.js",
  "repository": "https://github.com/mikerourke/toggl-to-clockify-web.git",
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "yarn run tidy",
    "build": "parcel build public/index.html --out-dir build --no-source-maps",
    "start:web": "parcel serve public/index.html --port 3000 --out-dir build",
    "start:server": "nodemon ./server/index.js",
    "start": "concurrently --kill-others \"yarn start:server\" \"yarn start:web\"",
    "lint": "eslint -c .eslintrc.json --ext .ts,.tsx src",
    "prettier": "prettier --write \"{src,config}/**/*.{js,ts,tsx}\"",
    "tidy": "rimraf ./.cache && rimraf ./build",
    "typecheck": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "clockify:delete": "node ./scripts/manageClockify.js delete",
    "clockify:write": "node ./scripts/manageClockify.js write",
    "test": "node ./scripts/runTests.js --env=jsdom --coverageReporters=lcov --config jest.config.json"
  },
  "dependencies": {
    "bloomer": "^0.6.5",
    "bulma": "^0.7.2",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "emotion": "^10.0.5",
    "fetch-intercept": "^2.3.1",
    "lodash": "^4.17.11",
    "normalizr": "^3.3.0",
    "promise-throttle": "^1.0.0",
    "react": "^16.8.4",
    "react-albus": "^2.0.0",
    "react-dom": "^16.8.4",
    "react-if": "^3.1.3",
    "react-redux": "^6.0.0",
    "react-select": "^2.4.3",
    "react-tooltip": "^3.9.0",
    "react-virtualized": "^9.21.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "store": "^2.0.12",
    "typesafe-actions": "^3.2.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-json-strings": "^7.2.0",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.11",
    "@types/jest-in-case": "^1.0.1",
    "@types/lodash": "^4.14.119",
    "@types/react": "^16.7.17",
    "@types/react-albus": "^2.0.4",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.3",
    "@types/react-select": "^2.0.17",
    "@types/react-tooltip": "^3.9.0",
    "@types/react-virtualized": "^9.18.11",
    "@types/store": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.0",
    "dotenv": "^7.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-unicorn": "^8.0.2",
    "esm": "^3.2.22",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "jest": "^24.3.1",
    "jest-in-case": "^1.0.2",
    "jsonfile": "^5.0.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.3.0",
    "nodemon": "^1.18.11",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "typescript": "^3.2.2",
    "yargs": "^13.2.2"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "babel": {
    "plugins": [
      "lodash",
      [
        "module-resolver",
        {
          "alias": {
            "^~/(.+)": "./src/\\1"
          }
        }
      ]
    ]
  }
}
