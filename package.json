{
  "name": "toggl-to-clockify-web",
  "version": "0.0.1",
  "description": "Web-based tool to transfer Toggl data to Clockify.",
  "main": "index.js",
  "repository": "https://github.com/mikerourke/toggl-to-clockify-web.git",
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "yarn tidy",
    "build": "parcel build ./src/index.html --out-dir build --no-source-maps --no-cache",
    "start:web": "parcel serve src/index.html --port 3000 --no-source-maps --no-cache",
    "start:server": "nodemon ./server/index.js",
    "start": "concurrently --kill-others \"yarn start:server\" \"yarn start:web\"",
    "lint": "eslint -c .eslintrc.json --ext .ts,.tsx src",
    "prettier": "prettier --write \"{scripts,src,server}/**/*.{js,ts,tsx,css,html}\"",
    "tidy": "rimraf ./.cache && rimraf ./build && rimraf ./dist",
    "typecheck": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "clockify:delete": "node ./scripts/manageClockify.js delete",
    "clockify:write": "node ./scripts/manageClockify.js write",
    "test": "node ./scripts/runTests.js --env=jsdom --coverageReporters=lcov --config jest.config.json"
  },
  "dependencies": {
    "bloomer": "^0.6.5",
    "bulma": "^0.8.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.8.1",
    "emotion": "^10.0.5",
    "fetch-intercept": "^2.3.1",
    "lodash": "^4.17.14",
    "normalizr": "^3.3.0",
    "promise-throttle": "^1.0.1",
    "react": "^16.12.0",
    "react-albus": "^2.0.0",
    "react-dom": "^16.12.0",
    "react-if": "^3.1.3",
    "react-redux": "^7.1.3",
    "react-select": "^3.0.4",
    "react-sweet-progress": "^1.1.2",
    "react-tooltip": "^3.9.0",
    "react-virtualized": "^9.21.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "store": "^2.0.12",
    "typesafe-actions": "^5.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-syntax-json-strings": "^7.7.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^24.0.23",
    "@types/jest-in-case": "^1.0.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.16",
    "@types/react-albus": "^2.0.4",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.0.3",
    "@types/react-select": "^3.0.0",
    "@types/react-tooltip": "^3.9.0",
    "@types/react-virtualized": "^9.18.11",
    "@types/store": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "body-parser": "^1.18.3",
    "chalk": "^3.0.0",
    "concurrently": "^5.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-unicorn": "^14.0.1",
    "express": "^4.16.4",
    "fs-extra": "^8.0.0",
    "jest": "^24.9.0",
    "jest-in-case": "^1.0.2",
    "jsonfile": "^5.0.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.3.0",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3",
    "yargs": "^15.0.2"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "babel": {
    "plugins": [
      "lodash",
      [
        "module-resolver",
        {
          "alias": {
            "^~/(.+)": "./src/\\1"
          }
        }
      ]
    ]
  }
}
